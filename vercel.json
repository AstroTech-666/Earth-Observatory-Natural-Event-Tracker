{
  "version": 2,
  "builds": [
    {
      "src": "eonat_dashboard.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "eonat_dashboard.py": {
      "memory": 512,
      "maxDuration": 10,
      "maxLambdaSize": 50
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "eonat_dashboard.py"
    }
  ]
}
